{
    "manifest_version" : 2,
    "name" : "Calculator",
    "version" : "1.0",
    "description" : "Calculate Anywhere",
    "icons" : {
        "128" : "icons128.png",
        "48" : "icons48.png",
        "16" : "icons16.png"
    },
    "browser_action" : {
        "default_icon" : "icons16.png",
        "default_popup" : "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}